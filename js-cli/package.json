{
  "name": "ts-cli",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "browserslist":{
    "development": [
      "last 1 version"
    ],
    "production": [
      "iOS >= 8",
      "Android >= 4"
    ]
  },
  "scripts": {
    "start": "cross-env RUNTIME=true BROWSERSLIST_ENV=development ENV=dev webpack serve --config webpack.config/runtime.js",
    "build:dev": "cross-env ENV=dev webpack --config webpack.config/dev.js",
    "build:test": "cross-env ENV=test webpack --config webpack.config/test.js",
    "build:prod": " cross-env ENV=prod webpack  --config webpack.config/prod.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@types/css-modules": "^1.0.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/webpack-env": "^1.16.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "postcss-loader": "^5.3.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
